{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "previewPath": null,
      "fields": [
        {
          "title": "Title",
          "name": "title",
          "type": "string",
          "required": false
        },
        {
          "title": "Description",
          "name": "desc",
          "type": "string",
          "required": false
        },
        {
          "title": "Page Path",
          "name": "path",
          "type": "string",
          "required": false
        },
        {
          "title": "Preview Image",
          "name": "preview",
          "type": "image"
        },
        {
          "title": "Published",
          "name": "published",
          "type": "draft"
        },
        {
          "title": "Last Modified",
          "name": "modified",
          "type": "datetime"
        },
        {
          "title": "Banner",
          "name": "banner",
          "type": "fields",
          "fields": [
            {
              "title": "Type",
              "name": "type",
              "type": "choice",
              "choices": [
                "error",
                "warn",
                "info"
              ]
            },
            {
              "title": "Behaviour",
              "name": "behaviour",
              "type": "choice",
              "choices": [
                "persistent"
              ]
            },
            {
              "title": "Content",
              "name": "content",
              "type": "string"
            }
          ]
        }
      ]
    }
  ],
  "frontMatter.framework.id": "other",
  "frontMatter.content.publicFolder": "[[workspace]]/static",
  "frontMatter.content.pageFolders": [
    {
      "title": "Content",
      "path": "[[workspace]]/src/content"
    }
  ],
  "frontMatter.content.draftField": {
    "name": "published",
    "type": "boolean",
    "invert": true
  },
  "frontMatter.taxonomy.tags": [],
  "frontMatter.taxonomy.categories": []
}